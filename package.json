{
  "name": "thinking-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^5.22.7",
    "axios": "^1.7.8",
    "echarts": "^5.6.0",
    "echarts-for-react": "^3.0.2",
    "express": "^5.1.0",
    "moment": "^2.30.1",
    "pinyin-pro": "^3.26.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.1.1",
    "react-scripts": "5.0.1",
    "react-virtualized": "^9.22.5",
    "react-window": "^1.8.10",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:dev": "NODE_ENV=development DEV_API_HOST=http://localhost:18888 react-scripts start",
    "start:prod": "NODE_ENV=production PROD_API_HOST=http://118.190.147.162:13000 react-scripts start",
    "start:test": "NODE_ENV=test TEST_API_HOST=http://test-server:13000 react-scripts start",
    "build:dev": "NODE_ENV=development DEV_API_HOST=http://localhost:18888 react-scripts build",
    "build:prod": "NODE_ENV=production PROD_API_HOST=http://118.190.147.162:13000 react-scripts build",
    "build:test": "NODE_ENV=test TEST_API_HOST=http://test-server:13000 react-scripts build",
    "electron-dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "npm run build && electron-builder -m",
    "electron-build": "electron-builder -m"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.stock.app",
    "mac": {
      "category": "public.app-category.finance",
      "target": "dmg"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "public/electron.js"
    ]
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "electron": "^33.3.1",
    "electron-builder": "^25.1.8",
    "wait-on": "^8.0.2"
  }
}
